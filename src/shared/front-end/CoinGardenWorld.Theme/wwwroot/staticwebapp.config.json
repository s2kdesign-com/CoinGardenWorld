{
  "forwardingGateway": {
    "allowedForwardedHosts": [
      "coingarden.world",
      "app.coingarden.world",
      "store.coingarden.world",
      "bot.coingarden.world",
      // Site for the metaverse
      "metaverse.coingarden.world",
      // The actual game
      "play.coingarden.world",
      "charity.coingarden.world"
    ]
  },
  "auth": {
    "identityProviders": {
      "customOpenIdConnectProviders": {
        "aadb2c": {
          "registration": {
            "clientIdSettingName": "AADB2C_PROVIDER_CLIENT_ID",
            "clientCredential": {
              "clientSecretSettingName": "AADB2C_PROVIDER_CLIENT_SECRET"
            },
            "openIdConnectConfiguration": {
              "wellKnownOpenIdConfiguration": "https://coingardenworld.b2clogin.com/coingardenworld.onmicrosoft.com/B2C_1_SignUpSignInLandingPage/v2.0/.well-known/openid-configuration"
            }
          },
          "login": {
            "nameClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
            "scopes": [],
            "loginParameterNames": []
          }
        }
      }
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/images/*.{png,jpg,gif}", "/_content/CoinGardenWorld.Theme/assets/*" ]
  },
  "trailingSlash": "auto",
  "responseOverrides": {
    "400": {
      "rewrite": "/invalid-invitation-error.html"
    },
    "401": {
      "redirect": "/login",
      "statusCode": 302
    },
    "404": {
      "rewrite": "/404.html"
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "routes": [
    {
      "route": "/profile*",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/admin/index.html",
      "allowedRoles": [ "administrator" ]
    },
    {
      "route": "/images/*",
      "headers": {
        "cache-control": "must-revalidate, max-age=15770000"
      }
    },
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "must-revalidate, max-age=15770000"
      }
    },
    {
      "route": "/_framework/*",
      "headers": {
        "cache-control": "must-revalidate, max-age=15770000"
      }
    }
  ]
}